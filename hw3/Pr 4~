Part a)

int bytes_read=0;
char bytes[10];//buffer for bytes
while (bytes_read<10){

	if (Value read from R1==1){
            store value read from R3 into bytes[bytes_read]
	    ++bytes_read;
	    write 1 to R2//notifies wifi card byte has been read
	}
	else
	    wait for R1 to be equal to 1
}
